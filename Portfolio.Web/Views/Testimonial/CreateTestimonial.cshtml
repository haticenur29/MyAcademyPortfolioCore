@model Testimonial
@{
    ViewData["Title"] = "Yeni Referans Oluştur";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">
    <div class="form-group">
        <label>Resim Url</label>
        <input type="text" class="form-control" asp-for="ImageUrl" />
    </div>
    <div class="form-group">
        <label>Ad Soyad</label>
        <input type="text" class="form-control" asp-for="Name" />
    </div>
    <div class="form-group">
        <label>Unvan</label>
        <input type="text" class="form-control" asp-for="Title" />
    </div>
    <div class="form-group">
        <label>Yorum</label>
        <input type="text" class="form-control" asp-for="Comment" />
    </div>
    <div class="form-group">
        <label>Yıldız</label>
        <input type="text" class="form-control" asp-for="Review" />
    </div>
    <button class="btn btn-outline-primary">Kaydet</button>
</form>

