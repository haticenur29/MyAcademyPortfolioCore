@model List<Education>
@*
	Default Education Component (Resume içindeki)
*@
<div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">

    <h3 class="resume-title">Eğitim</h3>

    @foreach (var education in Model)
    {
        <div class="resume-item">
            <h4>@education.SchoolName</h4>
            <h5>@education.Department</h5>
            <p><em>@education.StartYear - @education.EndYear</em></p>
            
        </div>
    }


    

   

</div>