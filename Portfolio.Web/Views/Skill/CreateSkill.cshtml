@model Skill
@{
    ViewData["Title"] = "Yeni Yetenek Oluştur";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}

<form method="post">
    <div class="form-group">
        <label>Yetenek Adı</label>
        <input type="text" class="form-control" asp-for="Title" />
    </div>
    <div class="form-group">
        <label>Yetenek Seviyesi</label>
        <input type="text" class="form-control" asp-for="Percentage" />
    </div>
    <button class="btn btn-outline-primary">Kaydet</button>
</form>

